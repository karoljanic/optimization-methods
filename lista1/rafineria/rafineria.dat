destylacja2     0.10       0.35    0.20        0.25
kraking         0.50       0.20    0.00        0.06;

param koszty_produkcji :=
    destylacja1 10
    destylacja2 10
    kraking 20;

param przeplyw_do_zbiornikow := 
[* , *, B1] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     0                   0                0
destylacja2     0                   0                0
kraking         0                   0                0
[* , *, B2] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     0                   0                0
destylacja2     0                   0                0
kraking         0                   0                0
[* , *, benzyna] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     1                   0                0
destylacja2     1                   0                0
kraking         1                   0                0

[* , *, olej] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     0                   1                1
destylacja2     0                   1                1
