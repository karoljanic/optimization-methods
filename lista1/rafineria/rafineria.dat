[* , *, destylat] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     0                   0                1
destylacja2     0                   0                1
kraking         0                   0                0

[* , *, resztki] :
                paliwoSilnikowe     paliwoDomowe     paliwoCiezkie :=
destylacja1     0                   0                1
destylacja2     0                   0                1
kraking         0                   0                1;

param limit_siarki_olejow_opalowych := 0.005;
param zawartosc_siarki_destylacji :=
    B1 0.002
    B2 0.012;

param zawartosc_siarki_krakingu :=
    B1 0.003
    B2 0.025;

param zapotrzebowanie := 
    paliwoSilnikowe 200000
    paliwoDomowe 400000
    paliwoCiezkie 250000;

end;
